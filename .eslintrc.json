{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "rules": {
    "no-console": "off",
    "indent": ["error", 2],
    "linebreak-style": ["error", "windows"],
    "semi": ["warn", "always"],
    "no-unused-vars": ["warn", { "vars": "local", "args": "after-used", "ignoreRestSiblings": true }]
  }
}
